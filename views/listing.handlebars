<div class="blog-listing">
  <h2>
    Recent posts:
  </h2>
  <div class="blog-post">
    {{#each posts as |post|}}
    <div>
      <h2>{{post.post_title}}</h2>
      <p>{{post.author_id}}</p>
      <p>{{post.post_body}}</p>
      
    </div>
    
    {{/each}}
  </div>
  <p>End of posts</p>
</div>
