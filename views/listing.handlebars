<div class="blog-listing">
  <h2>
    Recent posts:
  </h2>
  <div class="blog-post">
    {{#each posts as |post|}}
    <div>
      <a href="/posts/{{post.id}}"><h2>{{post.post_title}}</h2></a>
      <p>{{post.author.user_name}}</p>
      <p>{{post.post_body}}</p>
      
    </div>
    
    {{/each}}
  </div>
  <p>End of posts</p>
</div>
